{
    "author": "Andrei Petcu",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "description": "A WebExtension that redirects Deadspin.com links to the Wayback Machine archive. Icon is https://www.flaticon.com/free-icon/wikipedia_326943",
    "homepage_url": "https://github.com/andreicristianpetcu/mobile_wikipedia",
    "manifest_version": 2,
    "name": "Deadspin to Wayback Machine",
    "permissions": [
        "*://*.deadspin.com/*",
        "*://web.archive.org/*",
        "webRequest",
        "webRequestBlocking"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "deadspin-to-wayback@example.com"
        }
    },
    "version": "1.1.2"
}